class Internship {
    private String name;
    private double pts;

    public Internship(String name, double pts) {
        this.name = name;
        this.pts = pts;
    }

    public double getPts() {
        return this.pts;
    }

    public String getName() {
        return this.name;
    }

    @Override
    public String toString() {
        return String.format("\nInternship: %s\nPoints provided: %.1f", this.name, this.pts);
    }
}
